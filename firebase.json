{
	"hosting": {
		"public": "dist",
		"ignore": [
			"firebase.json"
		],
		"headers": [
			{
				"source": "**/*",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "no-transform,public,max-age=31536000"
					}
				]
			},
			{
				"source": "sw.js",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "no-cache"
					}
				]
			},
			{
				"source": "index.html",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "no-cache"
					}
				]
			}
		]
	}
}
